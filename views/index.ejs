    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link rel="icon" href="<%= url_prefix %>/static/img/favicon.ico">
    <link rel="stylesheet" href="<%= url_prefix %>/static/css/bootstrap.min.css">
    <link rel="stylesheet" href="<%= url_prefix %>/static/css/index.css">
    <title>MMovie</title>
